<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragment_home">

    <fragment
        android:id="@+id/fragment_home"
        android:name="com.teamkkumul.feature.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_fragment_home_to_homeDetailFragment"
            app:destination="@id/fragment_home_detail"
            app:popUpTo="@id/fragment_home"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_fragment_home_to_exampleComposeFragment"
            app:destination="@id/exampleComposeFragment" />
    </fragment>

    <fragment
        android:id="@+id/fragment_my_group"
        android:name="com.teamkkumul.feature.mygroup.MyGroupFragment"
        android:label="MyGroup"
        tools:layout="@layout/fragment_my_group" />

    <fragment
        android:id="@+id/fragment_my_page"
        android:name="com.teamkkumul.feature.mypage.MyPageFragment"
        android:label="MyPage"
        tools:layout="@layout/fragment_my_page" />
    <fragment
        android:id="@+id/fragment_home_detail"
        android:name="com.teamkkumul.feature.home.HomeDetailFragment"
        android:label="HomeDetailFragment" />
    <fragment
        android:id="@+id/exampleComposeFragment"
        android:name="com.teamkkumul.feature.compose.ExampleComposeFragment"
        android:label="ExampleComposeFragment" />

</navigation>
